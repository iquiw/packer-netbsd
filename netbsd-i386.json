      "iso_checksum_type": "sha512",
      "guest_additions_mode": "disable",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "shutdown_command": "su root -c '/sbin/shutdown -p now'",
      "boot_command": [
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait10s>",
        "<enter><enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait30s>",
        "<enter><wait10ms>",
        "a<enter><wait10ms><enter><wait10ms><enter><wait10ms><enter><wait10ms><wait15s>",
        "vagrant.local<enter><wait10ms><enter><wait10ms><enter><wait10ms>",
        "g<enter><wait10ms>",
        "o<enter><wait10ms>vagrant<enter><wait10ms><enter><wait10ms><enter><wait10ms>",
        "vagrant<enter><wait10ms>vagrant<enter><wait10ms>vagrant<enter><wait3s>",
        "x<enter><wait10ms>",
        "<enter><wait1s>",
        "d<enter><wait10ms>"
      ],
      "boot_wait": "40s"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "environment_vars": [
        "HOME_DIR=/home/vagrant"
      ],
      "scripts": [
        "scripts/vagrant.sh"
      ]
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "output": "netbsd-i386.box",
      "compression_level": 9
    }
