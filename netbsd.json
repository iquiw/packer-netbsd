{
  "builders": [
    {
      "type": "virtualbox-iso",
      "guest_os_type": "{{user `os_type`}}",
      "iso_url": "{{user `iso_url`}}",
      "iso_checksum": "sha512:{{user `iso_checksum`}}",
      "guest_additions_mode": "disable",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "shutdown_command": "su root -c '/sbin/shutdown -p now'",
      "boot_command": [
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait10ms>",
        "<down><enter><wait3s>",
        "<enter><wait10ms>",
        "<down><enter><wait10ms>",
        "<enter><wait50s>",
        "<enter><wait10ms>",
        "a<enter><wait10ms><enter><wait10ms><enter><wait10ms><enter><wait10ms><wait15s>",
        "<enter><wait10ms><enter><wait10ms>",
        "g<enter><wait10ms>",
        "o<enter><wait10ms>vagrant<enter><wait10ms><enter><wait10ms>b<enter><wait10ms>",
        "vagrant<enter><wait10ms>vagrant<enter><wait10ms>vagrant<enter><wait3s>",
        "x<enter><wait10ms>",
        "<enter><wait1s>",
        "d<enter><wait10ms>"
      ],
      "boot_wait": "30s"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "environment_vars": [
        "HOME_DIR=/home/vagrant"
      ],
      "scripts": [
        "scripts/vagrant.sh"
      ]
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "output": "netbsd-{{user `arch`}}.box",
      "compression_level": 9
    }
  ]
}
